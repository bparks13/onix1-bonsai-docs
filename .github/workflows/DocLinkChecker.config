{
  "DocumentationFiles": {
    "src": "",
    "Files": [
      "**/*.md"
    ],
    "Exclude": [
      "src",
      "images",
      "workflows"
    ]
  },
  "ResourceFolderNames": [
    "images",
    "workflows"
  ],
  "DocLinkChecker": {
    "RelativeLinkStrategy": "All",
    "CheckForOrphanedResources": true,
    "CleanupOrphanedResources": false,
    "ValidatePipeTableFormatting": true,
    "ValidateExternalLinks": true,
    "ConcurrencyLevel": 5,
    "MaxHttpRedirects": 20,
    "ExternalLinkDurationWarning": 3000,
    "WhitelistUrls": [
      "http://localhost"
    ]
  }
}
